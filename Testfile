pipeline {
  environment {
    dockerImage = ''
  }
    agent any 
    stages {
        stage('checkout') { 
            steps{
                git credentialsId: 'DocJen', url: 'git@github.com:PravinBalaji/Calendar-war.git'
            }
        }
        stage('Build code') { 
            steps{
              sh "mvn clean install" 
              }
            }
        }
}
